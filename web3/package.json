{
  "name": "burnie-web3-contracts",
  "version": "1.0.0",
  "description": "Smart contracts for Burnie Platform on Somnia Network",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:toast": "hardhat test test/TOASTToken.test.ts",
    "test:registry": "hardhat test test/ContentRegistry.test.ts",
    "test:rewards": "hardhat test test/ContentRewardDistribution.test.ts",
    "test:content": "hardhat test test/ContentRegistry.test.ts test/ContentRewardDistribution.test.ts",
    "test:workflow": "hardhat test test/FullWorkflow.test.ts",
    "workflow:somnia": "hardhat run scripts/runWorkflowOnSomnia.ts --network somniaTestnet",
    "demo:somnia": "hardhat run scripts/simpleSomniaDemo.ts --network somniaTestnet",
    "test:coverage": "hardhat coverage",
    "test:gas": "REPORT_GAS=true hardhat test",
    "clean": "hardhat clean",
    "typechain": "hardhat typechain",
    "node": "hardhat node",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "deploy:toast:testnet": "hardhat run scripts/deployTOAST.ts --network somniaTestnet",
    "deploy:toast:mainnet": "hardhat run scripts/deployTOAST.ts --network somniaMainnet",
    "deploy:toast:local": "hardhat run scripts/deployTOAST.ts --network hardhat",
    "deploy:content:testnet": "hardhat run scripts/deployContentRegistry.ts --network somniaTestnet",
    "deploy:content:mainnet": "hardhat run scripts/deployContentRegistry.ts --network somniaMainnet",
    "deploy:content:local": "hardhat run scripts/deployContentRegistry.ts --network hardhat",
    "deploy:rewards:testnet": "hardhat run scripts/deployRewardDistribution.ts --network somniaTestnet",
    "deploy:rewards:mainnet": "hardhat run scripts/deployRewardDistribution.ts --network somniaMainnet",
    "deploy:rewards:local": "hardhat run scripts/deployRewardDistribution.ts --network hardhat",
    "interact:toast:testnet": "hardhat run scripts/interactTOAST.ts --network somniaTestnet",
    "interact:toast:mainnet": "hardhat run scripts/interactTOAST.ts --network somniaMainnet",
    "interact:toast:local": "hardhat run scripts/interactTOAST.ts --network hardhat",
    "interact:content:testnet": "hardhat run scripts/interactContentContracts.ts --network somniaTestnet",
    "interact:content:mainnet": "hardhat run scripts/interactContentContracts.ts --network somniaMainnet",
    "interact:content:local": "hardhat run scripts/interactContentContracts.ts --network hardhat",
    "interact:registry:testnet": "hardhat run scripts/interactContentRegistry.ts --network somniaTestnet",
    "interact:registry:mainnet": "hardhat run scripts/interactContentRegistry.ts --network somniaMainnet",
    "interact:registry:local": "hardhat run scripts/interactContentRegistry.ts --network hardhat",
    "interact:rewards:testnet": "hardhat run scripts/interactRewardDistribution.ts --network somniaTestnet",
    "interact:rewards:mainnet": "hardhat run scripts/interactRewardDistribution.ts --network somniaMainnet",
    "interact:rewards:local": "hardhat run scripts/interactRewardDistribution.ts --network hardhat",
    "monitor:toast:testnet": "hardhat run scripts/monitorTOAST.ts --network somniaTestnet",
    "monitor:toast:mainnet": "hardhat run scripts/monitorTOAST.ts --network somniaMainnet",
    "verify:testnet": "hardhat verify --network somniaTestnet",
    "verify:mainnet": "hardhat verify --network somniaMainnet",
    "verify:base:testnet": "hardhat verify --network baseTestnet",
    "verify:base:mainnet": "hardhat verify --network baseMainnet",
    "verify:toast:testnet": "hardhat verify --network somniaTestnet 0x883bA39bA9b2d9724cC42715d6A105C3FA3A7578",
    "verify:toast:mainnet": "hardhat verify --network somniaMainnet <TOAST_ADDRESS>",
    "verify:content:testnet": "hardhat verify --network somniaTestnet 0xfc5FA67c369626c390dF165bED5D1aD60C9f4262 0x883bA39bA9b2d9724cC42715d6A105C3FA3A7578",
    "verify:content:mainnet": "hardhat verify --network somniaMainnet <CONTENT_REGISTRY_ADDRESS> <TOAST_ADDRESS>",
    "verify:rewards:testnet": "hardhat verify --network somniaTestnet 0x76237519643E2361FE197Ff255FbB5F2e4b48F7e 0x883bA39bA9b2d9724cC42715d6A105C3FA3A7578 0xDAFF70E39711564549C49b5CbD9F7938E709e03d 0xDAFF70E39711564549C49b5CbD9F7938E709e03d",
    "verify:rewards:mainnet": "hardhat verify --network somniaMainnet <REWARD_DISTRIBUTION_ADDRESS> <TOAST_ADDRESS> <EVALUATOR_TREASURY> <PLATFORM_TREASURY>",
    "console:testnet": "hardhat console --network somniaTestnet",
    "console:mainnet": "hardhat console --network somniaMainnet",
    "console:local": "hardhat console --network hardhat",
    "console:base:testnet": "hardhat console --network baseTestnet",
    "console:base:mainnet": "hardhat console --network baseMainnet",
    "network:info:testnet": "hardhat run scripts/networkInfo.ts --network somniaTestnet",
    "network:info:mainnet": "hardhat run scripts/networkInfo.ts --network somniaMainnet",
    "balance:check": "hardhat run scripts/checkBalance.ts --network somniaTestnet",
    "balance:check:mainnet": "hardhat run scripts/checkBalance.ts --network somniaMainnet",
    "explorer:monitor": "npm run monitor:toast:testnet",
    "explorer:info": "npm run network:info:testnet",
    "dev:setup": "npm run compile && npm run typechain",
    "dev:test": "npm run compile && npm run test",
    "dev:deploy": "npm run compile && npm run deploy:toast:testnet",
    "dev:full": "npm run dev:setup && npm run dev:test && npm run dev:deploy",
    "deploy:all:testnet": "npm run deploy:toast:testnet && npm run deploy:content:testnet && npm run deploy:rewards:testnet",
    "deploy:all:mainnet": "npm run deploy:toast:mainnet && npm run deploy:content:mainnet && npm run deploy:rewards:mainnet",
    "deploy:all:local": "npm run deploy:toast:local && npm run deploy:content:local && npm run deploy:rewards:local",
    "verify:all:testnet": "npm run verify:toast:testnet && npm run verify:content:testnet && npm run verify:rewards:testnet",
    "verify:all:mainnet": "npm run verify:toast:mainnet && npm run verify:content:mainnet && npm run verify:rewards:mainnet",
    "test:all:testnet": "npm run interact:toast:testnet && npm run interact:content:testnet",
    "test:all:mainnet": "npm run interact:toast:mainnet && npm run interact:content:mainnet",
    "test:all:local": "npm run interact:toast:local && npm run interact:content:local",
    "deploy:content:suite:testnet": "npm run deploy:content:testnet && npm run deploy:rewards:testnet && npm run interact:content:testnet",
    "deploy:content:suite:mainnet": "npm run deploy:content:mainnet && npm run deploy:rewards:mainnet && npm run interact:content:mainnet",
    "prod:deploy": "npm run compile && npm run deploy:toast:mainnet",
    "prod:verify": "npm run verify:mainnet",
    "prod:monitor": "npm run monitor:toast:mainnet",
    "prod:deploy:all": "npm run compile && npm run deploy:all:mainnet",
    "prod:verify:all": "npm run verify:all:mainnet",
    "prod:test:all": "npm run test:all:mainnet"
  },
  "keywords": [
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity",
    "erc20",
    "somnia",
    "burnie",
    "toast",
    "roast",
    "defi",
    "staking"
  ],
  "author": "Burnie Team",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-ignition": "^0.15.0",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": ">=9.1.0",
    "@types/node": ">=18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "chai": "^4.2.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "hardhat": "^2.22.0",
    "hardhat-gas-reporter": "^1.0.8",
    "prettier": "^3.0.0",
    "prettier-plugin-solidity": "^1.0.0",
    "solidity-coverage": "^0.8.1",
    "ts-node": ">=8.0.0",
    "typechain": "^8.3.0",
    "typescript": ">=4.5.0"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.4.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.0",
    "ethers": "^6.4.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/burnie-ai/burnie-platform.git",
    "directory": "web3"
  },
  "bugs": {
    "url": "https://github.com/burnie-ai/burnie-platform/issues"
  },
  "homepage": "https://burnie.io"
}
